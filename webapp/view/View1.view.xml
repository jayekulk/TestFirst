<mvc:View controllerName="odata.ODataModel.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Input id="input" value="{Northwind>/results/0/ProductName}"/>
					<List id="list" items="{}">
						<items>
							<ObjectListItem title="{Northwind>ProductName}"/>
						</items>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>